@model List<MoviesApp4.Models.MovieModel>
@{
    ViewBag.Title = "Movies List";
}

    <h2>Movies List</h2>


<table class="table table-striped">
    <tr>

        <th>Name</th>
        <th>Year </th>
        <th>CinemaID </th>
        <th>Kinoteatra nosaukums </th>


        <th></th>

    </tr>
    @foreach (var movies in Model)
    {
<tr>
    <th>@Html.ActionLink(movies.Name, "Edit", "Cinema", new { id = movies.Id }, null) </th>
    <th>
        @movies.Year



    </th>
    <th>
        @movies.CinemaId
    </th>
    <th>
        @movies.CinemaId

    </th>

</tr>
}
</table>
